<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.mirror.domain.Mdse" table="mdse">
		<id name="mdse_id" column="mdse_id">
			<generator class="native" />
		</id>
		
		<property name="mdse_name" column="mdse_name"/>
		<property name="mdse_status" column="mdse_status"/>
		<property name="mdse_addtime" column="mdse_addtime"/>
		<property name="mdse_mome" column="mdse_mome"/>
		<property name="mdse_price" column="mdse_price"/>
		<property name="mdse_imge" column="mdse_imge"/>
		<property name="mdse_quantity" column="mdse_quantity"/>
		<property name="mdse_sales" column="mdse_sales"/>
		
		<many-to-one name="user" class="com.mirror.domain.User" column="mdse_user_id"/>
		<many-to-one name="sort" class="com.mirror.domain.Sort" column="mdse_sort_id"/>
		<set name="order" inverse="true" cascade="delete">
    			<key column="order_mdse_id"></key>
    			<one-to-many class="com.mirror.domain.Order" />
    		</set>
	</class>
</hibernate-mapping>