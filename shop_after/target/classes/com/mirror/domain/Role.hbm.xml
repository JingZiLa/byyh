<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.mirror.domain.Role" table="role">
		<id name="user_role_id" column="user_role_id">
			<generator class="native" />
		</id>

		<property name="user_role_name" column="user_role_name" />

		<set name="user" inverse="true" cascade="delete">
			<key column="role_user_id"></key>
			<one-to-many class="com.mirror.domain.User" />
		</set>
	</class>
</hibernate-mapping>